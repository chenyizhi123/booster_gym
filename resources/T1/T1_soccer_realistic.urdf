<?xml version="1.0" encoding="utf-8"?>
<robot name="T1_Soccer_Realistic">
  <!-- 世界链接 -->
  <link name="world"/>

  <!-- 足球场地面 -->
  <link name="field_base">
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <mass value="0" />
      <inertia ixx="0" ixy="0" ixz="0" iyy="0" iyz="0" izz="0" />
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <box size="12 8 0.01" />
      </geometry>
      <material name="green">
        <color rgba="0.0 0.7 0.0 1.0" />
      </material>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <box size="12 8 0.01" />
      </geometry>
    </collision>
  </link>
  <joint name="world_to_field" type="fixed">
    <parent link="world"/>
    <child link="field_base"/>
    <origin xyz="0 0 0" rpy="0 0 0"/>
  </joint>

